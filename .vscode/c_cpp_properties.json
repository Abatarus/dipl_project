{
  "configurations": [
      {
          "name": "Win32",
          "includePath": [
              "${workspaceFolder}/**",
              "${env:USERPROFILE}/.conan2/**"
          ],
          "defines": [
              "_DEBUG",
              "UNICODE",
              "_UNICODE"
          ],
          "windowsSdkVersion": "10.0.22621.0",
          "compilerPath": "C:/Path/To/cl.exe",  // Убедитесь, что этот путь правильный
          "cStandard": "c17",
          "cppStandard": "c++17",
          "intelliSenseMode": "windows-msvc-x64",
          "browse": {
              "path": [
                  "${env:USERPROFILE}/.conan2/**",
                  "${workspaceFolder}/**"
              ],
              "limitSymbolsToIncludedHeaders": true,
              "databaseFilename": ""
          }
      }
  ],
  "version": 4
}